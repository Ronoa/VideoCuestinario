.contenedorPrincipal
  width: 100%
  height: 1070px;
  background:#E5E5E5
  z-index: -1 !important

.header
  margin-top : 20px;


.content
  background: rgb(0, 25, 91)
  height:200px
  margin-bottom: -120px
  h3
    color white
    font-weight: bold;
    padding: 30px 30px

.presentacion
  background: rgb(209, 75, 0)
  margin: 0 auto;
  height: auto;
  width: 100%
  max-width: 800px
  z-index: 100000
  h1
    color white
  img
    width: 100%

video
  width: 100%
  max-width: 100%


.nav
  background: rgb(255, 255, 255)
  text-align: right;
  padding: 12px  0 0 0
  .skip
    border-radius: 5px
    background: rgb(231, 231, 231)
    padding: 6px 42px
    color rgb(5, 71, 139)
    font-weight: bold
    font-size: 18px
    &:hover
      background: rgb(209, 75, 0)
      color black
