.contenedorPrincipal
  width: 100%
  height: 1070px;
  background:#E5E5E5

.header
  text-align: left
  height: 80px;
  width: 100%;
  background-color: #FFFFFF;
  box-shadow: 0 2px 2px 0 rgba(0,0,0,0.07);
  h1, h2, h3, h4
    margin: 0
    padding: 0
    line-height: 1.3em
    color #262626


.divuno
  width: 760px;
  height: 542px;
  margin-left: 250px
  margin-top:0px


.presentacion-video
  background: rgb(250, 250, 250)
  margin: 0 auto;
  height: auto;
  width: 100%
  max-width: 800px
  z-index: 100000
  padding: 50px
  h1
    color black
  img
    width: 100%

video
  width: 100%
  max-width: 100%

.back-quest,
.next-quest
  font-weight: bold


.back-quest
  a
    float: left
    text-align: center
    width: 150px
    display: block;
    background: rgb(226, 226, 226)
    padding: 10px 30px
.next-quest
  a
    text-align: center
    float: right
    width: 150px
    display: block;
    background: rgb(226, 226, 226)
    padding: 10px 30px
